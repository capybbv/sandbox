<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

    <!--
        Added the entity Document.
    -->
    <changeSet id="20231227081749-1" author="capybbv">
        <createTable tableName="document">
            <column name="document_id" type="bigint">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="document_name" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="employee_employee_id" type="bigint">
                <constraints nullable="true" />
            </column>
            <column name="document_type_document_type_id" type="bigint">
                <constraints nullable="true" />
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>
